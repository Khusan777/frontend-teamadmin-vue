v>\n    <div class=\"col-span-2\">\n      <at-select :options=\"selectOptions('Все команды', teams)\" v-model=\"query.team_id\" placeholder=\"Выберите команду\">\n      </at-select>\n    </div>\n    <div class=\"col-span-2\">\n      <AtSelect\n        :options=\"[\n          { title: 'weekly', value: 'weekly' },\n          { title: 'monthly', value: 'monthly' },\n        ]\"\n        v-model=\"query.period\"\n      ></AtSelect>\n    </div>\n    <div class=\"col-span-9\">\n      <div v-if=\"loading\">\n        <AtTable class=\"max-h-96 overflow-y-auto\" :elements=\"loadingArray\">\n          <template #element>\n            <at-table-item>\n              <at-skeleton type=\"row\"></at-skeleton>\n            </at-table-item>\n          </template>\n        </AtTable>\n      </div>\n      <div v-else>\n        <at-table :head=\"messagesTableHead\" :elements=\"office\">\n          <template #element=\"{ element }\">\n            <at-table-item :to=\"{ name: 'colleagues.detail', params: { id: element?.id } }\">\n              <p class=\"text-black whitespace-nowrap\">\n                {{ element?.name || '-' }}\n              </p>\n            </at-table-item>\n            <at-table-item :to=\"{ name: 'colleagues.detail', params: { id: element?.id } }\">\n              <p v-if=\"element.events_diff > 0\" class=\"text-black whitespace-nowrap items-center flex\">\n                {{ convertMinsToHrsMins(element?.worked_hour) || '-' }}\n              </p>\n              <p v-else class=\"text-black whitespace-nowrap items-center flex\">\n                {{ convertMinsToHrsMins(element?.worked_hour) || '-' }}\n              </p>\n              <p class=\"text-gray-500 text-sm whitespace-nowrap\">\n                пред. неделя: {{ convertMinsToHrsMins(element?.worked_hour) || '-' }}\n              </p>\n            </at-table-item>\n            <at-table-item :to=\"{ name: 'colleagues.detail', params: { id: element?.id } }\">\n              <p class=\"whitespace-nowrap\">\n                <span class=\"text-gray-400 text-sm\">от</span>\n                {{ formatDateByDayMonthYear(element?.from) || '-' }}\n              </p>\n              <p class=\"text-gray-500 whitespace-nowrap\">\n                <span class=\"text-gray-400 text-sm\">до {{ formatDateByDayMonthYear(element?.to) || '-' }}</span>\n              </p>\n            </at-table-item>\n            <p class=\"mt-5\">\n              <chevron-right-icon class=\"w-6 h-6 mt-8 text-gray-200\"></chevron-right-icon>\n            </p>\n          </template>\n          <template #footer>\n            <div class=\"flex justify-between items-center\">\n              <p class=\"justify-start text-gray-400\">\n                Показано от <span>{{ indexPagination?.from || '0' }}</span> до\n                <span> {{ indexPagination?.to || '0' }}</span> из\n                <span>{{ indexPagination?.total || '0' }}</span>\n              </p>\n            </div>\n            <div v-if=\"!office.length\" class=\"text-center my-5\">\n              <p>Пусто</p>\n            </div>\n          </template>\n        </at-table>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\n  import AtTable from '~/plugins/aliftech-ui/components/AtTable/AtTable';\n  import AtTableItem from '~/plugins/aliftech-ui/components/AtTableItem/AtTableItem';\n  import AtSkeleton from '~/plugins/aliftech-ui/components/AtSkeleton/AtSkeleton';\n  import AtSelect from '~/plugins/aliftech-ui/components/AtSelect/AtSelect';\n  import AtInput from '~/plugins/aliftech-ui/components/AtInput/AtInput';\n  import { formatDateByDayMonthYear } from '~/utils/filters/dateFilters';\n  import debounce from 'lodash.debounce';\n  import { ChevronRightIcon } from '@heroicons/vue/outline';\n  import { useStore } from 'vuex';\n  import { useRoute } from 'vue-router';\n  import useRouterReplace from '~/utils/hooks/useRouterReplace';\n  import { computed, ref, watch } from 'vue';\n\n  export default {\n    name: 'GitlabTasks',\n    components: {\n      AtTable,\n      AtTableItem,\n      AtSkeleton,\n      AtSelect,\n      AtInput,\n      ChevronRightIcon,\n    },\n    props: {\n      loading: Boolean,\n      officeActive: Array,\n      indexPagination: Object,\n    },\n    setup(props) {\n      const messagesTableHead = [{ title: 'ФИО' }, { title: 'ОПОЗДАНИЯ' }, { title: 'ПЕРИОД' }, { title: '' }];\n\n      const store = useStore();\n      const route = useRoute();\n      const routerReplace = useRouterReplace();\n      const loadingArray = ref(Array(15).fill(1));\n      const query = ref({\n        search: route.query.search || '',\n        department_id: route.query.department_id || '',\n        team_id: route.query.team_id || '',\n        page: route.query.page || '',\n        period: 'weekly' || route.query.period,\n        per_page: 10,\n      });\n\n      const office = computed(() => {\n        return props.officeActive;\n      });\n\n      const selectOptions = (defaultTitle, options) => {\n        return [{ title: defaultTitle, value: '' }, ...options];\n      };\n\n      watch(\n        query.value,\n        debounce(() => {\n          routerReplace.setRouter(query, {\n            page: 1,\n          });\n        }, 500)\n      );\n\n      const convertMinsToHrsMins = mins => {\n        let h = Math.floor(mins / 60);\n        let m = mins % 60;\n        h = h < 10 ? '0' + h : h; // (or alternatively) h = String(h).padStart(2, '0')\n        m = m < 10 ? '0' + m : m; // (or alternatively) m = String(m).padStart(2, '0')\n        return `${h} час. ${m} мин.`;\n      };\n\n      return {\n        route,\n        routerReplace,\n        query,\n        office,\n        messagesTableHead,\n        formatDateByDayMonthYear,\n        loadingArray,\n        selectOptions,\n        ChevronRightIcon,\n        convertMinsToHrsMins,\n        positions: computed(() => store.getters['core/positions']),\n        departments: computed(() => store.getters['core/departments']),\n        teams: computed(() => store.getters['core/teams']),\n      };\n    },\n  };\n</script>\n\n<style scoped></style>\n"]}]}