onent, ref } from 'vue';\n  import { ChevronLeftIcon } from '@heroicons/vue/solid';\n  import { useRoute } from 'vue-router';\n  import { $toast } from '~/main';\n  import { activateSurvey, editSurveyById, getShowSurvey } from '~/services/core/survey';\n  import { required } from '@vuelidate/validators';\n  import useVuelidate from '@vuelidate/core';\n  import store from '~/store';\n  const AtInput = defineAsyncComponent(() => import('~/plugins/aliftech-ui/components/AtInput/AtInput'));\n  const AtSkeleton = defineAsyncComponent(() => import('~/plugins/aliftech-ui/components/AtSkeleton/AtSkeleton'));\n  const AtButton = defineAsyncComponent(() => import('~/plugins/aliftech-ui/components/AtButton/AtButton'));\n  const AtModal = defineAsyncComponent(() => import('~/plugins/aliftech-ui/components/AtModal/AtModal'));\n  const loading = ref(false);\n  const editNameQuestion = ref('');\n  const nameQuestion = ref('');\n  const editModalSurvey = ref(false);\n  const btnLoading = ref(false);\n  const typeQuestion = ref([]);\n  const startModalSurvey = ref(false);\n  const startSurvey = ref(0);\n  const route = useRoute();\n  const AtPanel = defineAsyncComponent(() => import('~/plugins/aliftech-ui/components/AtPanel/AtPanel'));\n  const AtSelect = defineAsyncComponent(() => import('~/plugins/aliftech-ui/components/AtSelect/AtSelect'));\n  const rulesEditSurvey = {\n    editNameQuestion: { required },\n    typeQuestion: { required },\n  };\n\n  const rules = {\n    nameQuestion: { required },\n    typeQuestion: { required },\n  };\n\n  const questionValidation = useVuelidate(rules, {\n    editNameQuestion,\n    typeQuestion,\n  });\n\n  const questionEditValidation = useVuelidate(rulesEditSurvey, {\n    editNameQuestion,\n    typeQuestion,\n  });\n  const surveyCategories = ref(store.getters['core/surveysCategory']);\n  const surveyData = ref({});\n\n  const getSurveyShowById = () => {\n    loading.value = true;\n    getShowSurvey(Number(route.params.id))\n      .then(response => {\n        surveyData.value = response?.data.data;\n        nameQuestion.value = response?.data.data.title;\n        typeQuestion.value = response?.data.data.category?.id;\n      })\n      .catch(error => {\n        if (error) {\n          $toast.show({ title: 'Error', subTitle: error?.response?.data?.message, type: 'error' });\n        }\n      })\n      .finally(() => (loading.value = false));\n  };\n  getSurveyShowById();\n\n  const editSurvey = async () => {\n    questionEditValidation.value.$reset();\n    if (questionEditValidation.value.$invalid) {\n      questionEditValidation.value.$touch();\n      return;\n    } else {\n      btnLoading.value = true;\n      await editSurveyById(route.params.id)\n        .then(() => {\n          $toast.show({\n            title: 'Успешно',\n            subTitle: 'Успешно отправлено',\n            type: 'success',\n          });\n        })\n        .catch(error => {\n          $toast.show({ title: 'Error', subTitle: error?.response?.data?.message, type: 'error' });\n        })\n        .finally(() => {\n          editModalSurvey.value = false;\n          btnLoading.value = false;\n        });\n      getSurveyShowById();\n    }\n  };\n\n  const activationSurvey = () => {\n    btnLoading.value = true;\n    activateSurvey(route.params.id, {\n      'is_active': startSurvey.value,\n    })\n      .then(() => {\n        startSurvey.value = 1;\n        $toast.show({\n          title: 'Успешно',\n          subTitle: 'Успешно запущено',\n          type: 'success',\n        });\n        getSurveyShowById();\n      })\n      .catch(error => {\n        $toast.show({ title: 'Error', subTitle: error?.data?.message, type: 'error' });\n      })\n      .finally(() => {\n        startModalSurvey.value = false;\n        btnLoading.value = false;\n      });\n  };\n</script>\n\n<template>\n  <div>\n    <div class=\"-m-6 mb-4\">\n      <div class=\"flex items-center p-3 border-b\">\n        <chevron-left-icon\n          class=\"w-10 h-18 text-gray-300 mr-2 ml-2 cursor-pointer\"\n          @click=\"$router.go(-1)\"\n        ></chevron-left-icon>\n        <h1 @click=\"$router.go(-1)\" class=\"text-2xl font-bold cursor-pointer\">{{ nameQuestion || '-' }}</h1>\n        <div class=\"flex flex-grow justify-end\">\n          <div v-if=\"startSurvey == 0\">\n            <at-button color=\"primary\" @click=\"startModalSurvey = true\" icon=\"play\">Запустить опрос</at-button>\n          </div>\n          <div v-else>\n            <at-button color=\"primary\" @click=\"startModalSurvey = true\" icon=\"stop\">Остановить опрос</at-button>\n          </div>\n        </div>\n      </div>\n      <div class=\"p-7 w-7/12 max-h-screen\">\n        <at-panel>\n          <template #header>\n            <div class=\"flex justify-between gap-x-4 items-center\">\n              <p class=\"text-lg text-md\">Информация</p>\n              <at-button @click=\"editModalSurvey = true\" color=\"secondary\">Изменить</at-button>\n            </div>\n          </template>\n          <div v-if=\"loading\">\n            <div class=\"mb-1 p-2\" v-for=\"i in 5\" :key=\"i\">\n              <at-skeleton type=\"row\"></at-skeleton>\n            </div>\n          </div>\n          <div v-else>\n            <div class=\"mb-4\">\n              <AtInput\n                disabled\n                label=\"Название\"\n                placeholder=\"Введите название опросника\"\n                :error=\"questionValidation.nameQuestion.$error\"\n                v-model=\"nameQuestion\"\n              />\n              <p class=\"text-red-500\" v-if=\"questionValidation.nameQuestion.$error\">Поле обязательно для заполнения</p>\n            </div>\n            <div class=\"mb-4\">\n              <AtSelect\n                disabled\n                label=\"Категория опросника\"\n                :options=\"surveyCategories\"\n                placeholder=\"Выберите категорию опросника\"\n                :error=\"questionValidation.typeQuestion.$error\"\n                v-model=\"typeQuestion\"\n              />\n              <p class=\"text-red-500\" v-if=\"questionValidation.typeQuestion.$error\">Поле обязательно для заполнения</p>\n            </div>\n          </div>\n        </at-panel>\n      </div>\n    </div>\n\n    <AtModal v-model=\"editModalSurvey\" :title=\"'Редактировать опрос'\">\n      <div class=\"mt-2\">\n        <AtInput\n          label=\"Название\"\n          placeholder=\"Введите название опросника\"\n          :error=\"questionEditValidation.editNameQuestion.$error\"\n          v-model=\"editNameQuestion\"\n        />\n        <p class=\"text-red-500\" v-if=\"questionEditValidation.editNameQuestion.$error\">\n          Поле обязательно для заполнения\n        </p>\n        <AtSelect\n          label=\"Категория\"\n          :options=\"surveyCategories\"\n          placeholder=\"Выберите категорию опросника\"\n          :error=\"questionEditValidation.typeQuestion.$error\"\n          v-model=\"typeQuestion\"\n        />\n        <p class=\"text-red-500\" v-if=\"questionEditValidation.typeQuestion.$error\">Поле обязательно для заполнения</p>\n        <div class=\"flex space-x-3 mt-3\">\n          <AtButton class=\"w-full p-4\" @click=\"editModalSurvey = false\">Закрыть</AtButton>\n          <AtButton class=\"w-full p-4\" color=\"primary\" :loading=\"btnLoading\" @click=\"editSurvey()\">\n            Сохранить\n          </AtButton>\n        </div>\n      </div>\n    </AtModal>\n\n    <AtModal v-model=\"startModalSurvey\" :title=\"'Запустить опрос'\">\n      <p class=\"text-center text-lg text-yellow-400\">\n        Вы действительно хотите запустить опрос <span class=\"text-black\"> {{ nameQuestion }}!!!</span>\n      </p>\n      <div class=\"flex space-x-3 mt-3\">\n        <AtButton class=\"w-full p-4\" @click=\"startModalSurvey = false\">Закрыть</AtButton>\n        <AtButton class=\"w-full p-4\" color=\"primary\" :loading=\"btnLoading\" @click=\"activationSurvey()\">\n          Сохранить\n        </AtButton>\n      </div>\n    </AtModal>\n  </div>\n</template>\n\n<style scoped></style>\n"],"sourceRoot":""}]}