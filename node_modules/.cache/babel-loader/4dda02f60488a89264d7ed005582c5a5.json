eA,KAAK,GAAG,EAA3B,EAA+B;AACpCE,IAAAA,SAAS,GAAG,MAAZ;AACD,GAFM,MAEA;AACLA,IAAAA,SAAS,GAAG,MAAZ;AACD;;AACD,mBAAUF,KAAV,cAAmBR,OAAO,CAACW,UAAR,EAAnB,cAA2CD,SAA3C,gBAA0DV,OAAO,CAACM,OAAR,EAA1D,cACEV,MAAM,CAAC,OAAD,CAAN,CAAgBM,aAAhB,CADF,cAEIF,OAAO,CAACO,WAAR,EAFJ;AAGD,CApBM","sourcesContent":["import months from '../constants/months';\n\n/**\n * Formatting incoming date by the time of the day / Форматирование входящей даты по времени суток\n * @param {string} date - Incoming date\n * @param {string} type - Date type\n * @returns string\n */\nexport const formatDateByTheTimeOfDay = (date, type = 'short') => {\n  const newDate = new Date(date);\n  const MM_DateFormat = Number.parseInt(newDate.getMonth(), 10);\n  if (type === 'short') {\n    return `${newDate.getDate()} ${months['short'][MM_DateFormat]} ${newDate.getFullYear()}`;\n  }\n  const hours = newDate.getHours();\n  let dayPeriod = '';\n  if (hours > 4 && hours < 12) {\n    dayPeriod = 'утра';\n  } else if (hours >= 12 && hours < 16) {\n    dayPeriod = 'дня';\n  } else if (hours >= 16 && hours < 22) {\n    dayPeriod = 'веч.';\n  } else {\n    dayPeriod = 'ночи';\n  }\n  return `${hours}:${newDate.getMinutes()} ${dayPeriod} - ${newDate.getDate()} ${\n    months['short'][MM_DateFormat]\n  } ${newDate.getFullYear()}`;\n};\n"]}]}