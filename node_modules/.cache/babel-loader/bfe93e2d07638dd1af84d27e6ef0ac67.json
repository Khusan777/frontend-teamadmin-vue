der=\"Поиск по ФИО\" iconAfter=\"search\" v-model=\"query.search\" />\n    </div>\n    <div class=\"sm:col-span-2 col-span-9\">\n      <at-select\n        :options=\"selectOptions('Все отделы', departments)\"\n        v-model=\"query.department_id\"\n        placeholder=\"Выберите отдел\"\n      >\n      </at-select>\n    </div>\n    <div class=\"sm:col-span-2 col-span-9\">\n      <at-select :options=\"selectOptions('Все команды', teams)\" v-model=\"query.team_id\" placeholder=\"Выберите команду\">\n      </at-select>\n    </div>\n    <div class=\"sm:col-span-2 col-span-9\">\n      <AtSelect\n        :options=\"[\n          { title: 'weekly', value: 'weekly' },\n          { title: 'monthly', value: 'monthly' },\n        ]\"\n        v-model=\"query.period\"\n      ></AtSelect>\n    </div>\n    <div class=\"col-span-9\">\n      <div v-if=\"loading\">\n        <at-skeleton type=\"row\" class=\"mb-2 p-5\"></at-skeleton>\n        <AtTable class=\"max-h-96 overflow-y-auto\" :elements=\"loadingArray\">\n          <template #element>\n            <at-table-item>\n              <at-skeleton type=\"row\"></at-skeleton>\n            </at-table-item>\n          </template>\n        </AtTable>\n      </div>\n      <div v-else>\n        <div v-if=\"allMetric.length\" class=\"col-span-9 mb-6\">\n          <at-alert type=\"info\">\n            <span class=\"text-blue-400 text-sm\">Результаты с</span>\n            <span class=\"text-blue-800 text-md\">{{ ' ' + formatDateByDayMonthYear(showFilterFromDate()) || '-' }}</span>\n            <span class=\"text-blue-400 text-sm\"> по</span>\n            <span class=\"text-blue-800 text-md\">{{ ' ' + formatDateByDayMonthYear(showFilterToDate()) || '-' }}</span>\n          </at-alert>\n        </div>\n        <at-table :head=\"messagesTableHead\" :elements=\"allMetric\">\n          <template #element=\"{ element }\">\n            <at-table-item :to=\"{ name: 'colleagues.detail', params: { id: element?.id } }\">\n              <p class=\"text-black whitespace-nowrap\">\n                {{ element?.name || '-' }}\n              </p>\n            </at-table-item>\n            <at-table-item :to=\"{ name: 'colleagues.detail', params: { id: element?.id } }\">\n              <linear-metric\n                :current_linear_issues_count=\"element?.current_linear_issues_solved_count\"\n                :previous_linear_issues_count=\"element?.previous_linear_issues_solved_count\"\n              ></linear-metric>\n            </at-table-item>\n            <at-table-item :to=\"{ name: 'colleagues.detail', params: { id: element?.id } }\">\n              <gitlab-metric\n                :current_gitlab_events_count=\"element?.current_gitlab_events_count\"\n                :previous_gitlab_events_count=\"element?.previous_gitlab_events_count\"\n              ></gitlab-metric>\n            </at-table-item>\n            <at-table-item :to=\"{ name: 'colleagues.detail', params: { id: element?.id } }\">\n              <wifi-metric\n                :current_wifi_late_minutes=\"element?.current_wifi_late_minutes\"\n                :previous_wifi_late_minute=\"element?.previous_wifi_late_minutes\"\n              ></wifi-metric>\n            </at-table-item>\n            <at-table-item :to=\"{ name: 'colleagues.detail', params: { id: element?.id } }\">\n              <office-metric\n                :current_office_activity_minutes=\"element?.current_office_activity_minutes\"\n                :previous_office_activity_minutes=\"element?.previous_office_activity_minutes\"\n              ></office-metric>\n            </at-table-item>\n            <at-table-item>\n              <manager-data-metric\n                :current_linear_issues_created_count=\"element?.current_linear_issues_created_count\"\n                :previous_linear_issues_created_count=\"element?.previous_linear_issues_created_count\"\n              >\n              </manager-data-metric>\n            </at-table-item>\n            <p class=\"mt-4\">\n              <chevron-right-icon class=\"w-10 h-12 text-gray-200\"></chevron-right-icon>\n            </p>\n          </template>\n          <template #footer>\n            <div class=\"flex justify-between items-center\">\n              <p class=\"justify-start text-gray-400\">\n                Показано от <span>{{ indexPagination.meta?.from || '0' }}</span> до\n                <span> {{ indexPagination.meta?.to || '0' }}</span> из\n                <span>{{ indexPagination.meta?.total || '0' }}</span>\n              </p>\n            </div>\n            <div v-if=\"!allMetric.length\" class=\"text-center my-5\">\n              <p>Пусто</p>\n            </div>\n          </template>\n        </at-table>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\n  import AtTable from '~/plugins/aliftech-ui/components/AtTable/AtTable';\n  import AtTableItem from '~/plugins/aliftech-ui/components/AtTableItem/AtTableItem';\n  import AtSkeleton from '~/plugins/aliftech-ui/components/AtSkeleton/AtSkeleton';\n  import AtSelect from '~/plugins/aliftech-ui/components/AtSelect/AtSelect';\n  import AtAlert from '~/plugins/aliftech-ui/components/AtAlert/AtAlert';\n  import AtInput from '~/plugins/aliftech-ui/components/AtInput/AtInput';\n  import { formatDateByDayMonthYear } from '~/utils/filters/dateFilters';\n  import { debounce } from '~/plugins/aliftech-ui/utils';\n  import { useStore } from 'vuex';\n  import { useRoute } from 'vue-router';\n  import useRouterReplace from '~/utils/hooks/useRouterReplace';\n  import { computed, ref, watch } from 'vue';\n  import LinearMetric from '~/components/productivity/LinearMetric';\n  import GitlabMetric from '~/components/productivity/GitlabMetric';\n  import WifiMetric from '~/components/productivity/WifiMetric';\n  import OfficeMetric from '~/components/productivity/OfficeMetric';\n  import ManagerDataMetric from '~/components/productivity/ManagerDataMetric';\n  import { ChevronRightIcon } from '@heroicons/vue/solid';\n\n  export default {\n    name: 'AllMetricsIndex',\n    components: {\n      OfficeMetric,\n      WifiMetric,\n      GitlabMetric,\n      LinearMetric,\n      ManagerDataMetric,\n      AtAlert,\n      AtTable,\n      AtTableItem,\n      AtSkeleton,\n      AtSelect,\n      ChevronRightIcon,\n      AtInput,\n    },\n    props: {\n      loading: Boolean,\n      indexPagination: Object,\n      indexMetric: Array,\n    },\n    setup(props) {\n      const messagesTableHead = [\n        { title: 'ФИО' },\n        { title: 'LINEAR ЗАДАЧ РЕШЕНО' },\n        { title: 'GITLAB' },\n        { title: 'WiFi подключение' },\n        { title: 'Активность в офисе' },\n        { title: 'LINEAR ЗАДАЧ СОЗДАНО' },\n      ];\n\n      const store = useStore();\n      const route = useRoute();\n      const routerReplace = useRouterReplace();\n      const loadingArray = ref(Array(15).fill(1));\n      const query = ref({\n        search: route.query.search || '',\n        department_id: route.query.department_id || '',\n        team_id: route.query.team_id || '',\n        page: route.query.page || '',\n        period: 'weekly' || route.query.period,\n        per_page: 15,\n      });\n\n      const selectOptions = (defaultTitle, options) => {\n        return [{ title: defaultTitle, value: '' }, ...options];\n      };\n\n      watch(\n        query.value,\n        debounce(() => {\n          routerReplace.setRouter(query, {\n            page: 1,\n          });\n        }, 500)\n      );\n\n      const allMetric = computed(() => {\n        return props.indexMetric;\n      });\n\n      const showFilterFromDate = () => {\n        for (let dateFromFilter of props.indexMetric) {\n          return dateFromFilter?.from_date;\n        }\n      };\n\n      const showFilterToDate = () => {\n        for (let dateFromFilter of props.indexMetric) {\n          return dateFromFilter?.to_date;\n        }\n      };\n\n      return {\n        allMetric,\n        route,\n        routerReplace,\n        query,\n        props,\n        messagesTableHead,\n        ChevronRightIcon,\n        formatDateByDayMonthYear,\n        loadingArray,\n        showFilterFromDate,\n        showFilterToDate,\n        selectOptions,\n        positions: computed(() => store.getters['core/positions']),\n        departments: computed(() => store.getters['core/departments']),\n        teams: computed(() => store.getters['core/teams']),\n      };\n    },\n  };\n</script>\n\n<style scoped></style>\n"],"sourceRoot":""}]}