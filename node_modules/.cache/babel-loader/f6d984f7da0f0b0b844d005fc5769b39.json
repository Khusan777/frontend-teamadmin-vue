D,OAPH,EAQG,KARH,CAQS,UAAA,KAAK,EAAI;AAAA;;AACd,QAAA,MAAM,CAAC,IAAP,CAAY;AAAE,UAAA,KAAK,EAAE,OAAT;AAAkB,UAAA,QAAQ,EAAE,KAAF,aAAE,KAAF,2CAAE,KAAK,CAAE,QAAT,8EAAE,iBAAiB,IAAnB,0DAAE,sBAAuB,OAAnD;AAA4D,UAAA,IAAI,EAAE;AAAlE,SAAZ;AACD,OAVH,EAWG,OAXH,CAWW,YAAM;AACb,QAAA,iBAAiB,CAAC,KAAlB,GAA0B,KAA1B;AACA,QAAA,UAAU,CAAC,KAAX,GAAmB,KAAnB;AACD,OAdH;AAeA,MAAA,aAAa;AACd,KAlBD","sourcesContent":["<script setup>\n  import { defineAsyncComponent, ref } from 'vue';\n  import useVuelidate from '@vuelidate/core';\n  import { required } from '@vuelidate/validators';\n  import { formatDateToLocaleString } from '~/utils/parsers';\n  import { $toast } from '~/main';\n  import { createSurvey, createSurveyCategory, deleteSurveyById, getIndexSurveys } from '~/services/core/survey';\n  import store from '~/store';\n  const surveys = ref([]);\n  const createModalQuestion = ref(false);\n  const btnLoading = ref(false);\n  const nameQuestion = ref('');\n  const nameCategorySurvey = ref('');\n  const typeQuestion = ref([]);\n  const createModalSurveyCategory = ref(false);\n  const deleteModalSurvey = ref(false);\n  const loading = ref(false);\n  const loadingArray = ref(Array(15).fill(1));\n  const surveyId = ref(Number);\n  const surveyCategory = ref(store.getters['core/surveysCategory']);\n  const AtButton = defineAsyncComponent(() => import('~/plugins/aliftech-ui/components/AtButton/AtButton.js'));\n  const AtModal = defineAsyncComponent(() => import('~/plugins/aliftech-ui/components/AtModal/AtModal.js'));\n  const AtSelect = defineAsyncComponent(() => import('~/plugins/aliftech-ui/components/AtSelect/AtSelect.js'));\n  const AtInput = defineAsyncComponent(() => import('~/plugins/aliftech-ui/components/AtInput/AtInput.js'));\n  const AtTable = defineAsyncComponent(() => import('~/plugins/aliftech-ui/components/AtTable/AtTable'));\n  const AtSkeleton = defineAsyncComponent(() => import('~/plugins/aliftech-ui/components/AtSkeleton/AtSkeleton'));\n  const AtTableItem = defineAsyncComponent(() => import('~/plugins/aliftech-ui/components/AtTableItem/AtTableItem'));\n\n  const rules = {\n    nameQuestion: { required },\n    typeQuestion: { required },\n  };\n  const questionValidation = useVuelidate(rules, {\n    nameQuestion,\n    typeQuestion,\n  });\n\n  const rulesSurveyCategory = {\n    nameCategorySurvey: { required },\n  };\n  const categorySurveyValidation = useVuelidate(rulesSurveyCategory, {\n    nameCategorySurvey,\n  });\n\n  const messagesTableHead = [\n    { title: '#' },\n    { title: 'НАЗВАНИЕ' },\n    { title: 'КОЛ-ВО УЧАСТНИКОВ' },\n    { title: 'ДАТА СОЗДАНИЯ' },\n    { title: '' },\n  ];\n\n  const getAllSurveys = () => {\n    loading.value = true;\n    getIndexSurveys()\n      .then(response => {\n        surveys.value = response?.data.data;\n      })\n      .catch(error => {\n        if (error) {\n          $toast.show({ title: 'Error', subTitle: error?.response?.data?.message, type: 'error' });\n        }\n      })\n      .finally(() => (loading.value = false));\n  };\n\n  getAllSurveys();\n\n  const addQuestion = async () => {\n    questionValidation.value.$reset();\n    if (questionValidation.value.$invalid) {\n      questionValidation.value.$touch();\n      return;\n    } else {\n      btnLoading.value = true;\n      await createSurvey({\n        'survey_category_id': typeQuestion.value,\n        'title': nameQuestion.value,\n        'description': nameQuestion.value,\n        'answers_count': null,\n      })\n        .then(() => {\n          $toast.show({\n            title: 'Успешно',\n            subTitle: 'Успешно отправлено',\n            type: 'success',\n          });\n        })\n        .catch(error => {\n          $toast.show({ title: 'Error', subTitle: error?.response?.data?.message, type: 'error' });\n        })\n        .finally(() => {\n          createModalQuestion.value = false;\n          btnLoading.value = false;\n        });\n      getAllSurveys();\n    }\n  };\n\n  const addSurveyCategory = async () => {\n    categorySurveyValidation.value.$reset();\n    if (categorySurveyValidation.value.$invalid) {\n      categorySurveyValidation.value.$touch();\n      return;\n    } else {\n      btnLoading.value = true;\n      await createSurveyCategory({\n        'title': nameCategorySurvey.value,\n      })\n        .then(() => {\n          $toast.show({\n            title: 'Успешно',\n            subTitle: 'Успешно отправлено',\n            type: 'success',\n          });\n          store.dispatch('core/getApp');\n        })\n        .catch(error => {\n          $toast.show({ title: 'Error', subTitle: error?.response?.data?.message, type: 'error' });\n        })\n        .finally(() => {\n          createModalSurveyCategory.value = false;\n          btnLoading.value = false;\n        });\n    }\n  };\n\n  const deleteSurvey = (surveyId) => {\n    btnLoading.value = true;\n    deleteSurveyById(surveyId)\n      .then(() => {\n        $toast.show({\n          title: 'Успешно',\n          subTitle: 'Успешно отправлено',\n          type: 'success',\n        });\n      })\n      .catch(error => {\n        $toast.show({ title: 'Error', subTitle: error?.response?.data?.message, type: 'error' });\n      })\n      .finally(() => {\n        deleteModalSurvey.value = false;\n        btnLoading.value = false;\n      });\n    getAllSurveys();\n  };\n</script>\n\n<template>\n  <div class=\"-m-6 mb-4\">\n    <div class=\"w-full bg-alpha\">\n      <div class=\"flex items-center justify-between p-4 border-b\">\n        <h1 class=\"text-2xl font-bold px-3\">Опросы</h1>\n        <div class=\"space-x-4\">\n          <AtButton @click=\"createModalQuestion = true\" icon=\"plus\" color=\"primary\">Создать опрос</AtButton>\n          <AtButton @click=\"createModalSurveyCategory = true\" icon=\"plus\" color=\"secondary\">Создать категорию</AtButton>\n        </div>\n      </div>\n    </div>\n  </div>\n  <div class=\"grid grid-cols-7\">\n    <div class=\"col-span-7\">\n      <div v-if=\"loading\">\n        <AtTable class=\"max-h-96 overflow-y-auto\" :elements=\"loadingArray\">\n          <template #element>\n            <at-table-item>\n              <at-skeleton type=\"row\"></at-skeleton>\n            </at-table-item>\n          </template>\n        </AtTable>\n      </div>\n      <div v-else>\n        <at-table :head=\"messagesTableHead\" :elements=\"surveys\">\n          <template #element=\"{ element, index }\">\n            <at-table-item>\n              <p class=\"text-gray-500\">{{ index + 1 }}</p>\n            </at-table-item>\n            <at-table-item class=\"max-w-md\" :to=\"{ name: 'survey.detail', params: { id: element?.id } }\">\n              <p class=\"text-black whitespace-nowrap\">\n                {{ element?.title || '-' }}\n              </p>\n            </at-table-item>\n            <at-table-item class=\"max-w-md\" :to=\"{ name: 'survey.detail', params: { id: element?.id } }\">\n              <p class=\"text-gray-500 whitespace-nowrap\">134</p>\n            </at-table-item>\n            <at-table-item class=\"max-w-md\" :to=\"{ name: 'survey.detail', params: { id: element?.id } }\">\n              <p class=\"whitespace-nowrap\">\n                {{ formatDateToLocaleString(element?.category?.created_at) || '-' }}\n              </p>\n              <p class=\"text-gray-500 text-sm ml-6\">\n                {{ $filters.readableTime(element?.category?.created_at) || '-' }}\n              </p>\n            </at-table-item>\n            <at-table-item>\n              <at-button\n                @click=\"(deleteModalSurvey = true), ((surveyId = element?.id), (nameQuestion = element?.title))\"\n                color=\"danger\"\n                icon=\"trash\"\n                >Удалить</at-button\n              >\n            </at-table-item>\n          </template>\n          <template #footer>\n            <div v-if=\"!surveys?.length\" class=\"text-center my-5\">\n              <p>Пусто</p>\n            </div>\n          </template>\n        </at-table>\n      </div>\n    </div>\n\n    <AtModal v-model=\"deleteModalSurvey\" :title=\"'Удалить текущий опрос'\">\n      <div class=\"mt-2 flex justify-center items-center\">\n        <p class=\"text-yellow-500 text-center text-lg\">\n          Вы точно хотите удалить этот опрос:<br />\n          <span class=\"text-black\"> {{ nameQuestion }}!!!</span>\n        </p>\n      </div>\n      <div class=\"flex space-x-3 mt-3\">\n        <AtButton class=\"w-full p-4\" @click=\"deleteModalSurvey = false\">Закрыть</AtButton>\n        <AtButton class=\"w-full p-4\" color=\"primary\" :loading=\"btnLoading\" @click=\"deleteSurvey(surveyId)\">Удалить</AtButton>\n      </div>\n    </AtModal>\n\n    <AtModal v-model=\"createModalSurveyCategory\" :title=\"'Создать новую категорию опросника'\">\n      <div class=\"mt-2\">\n        <AtInput\n          label=\"Название\"\n          placeholder=\"Введите название категории\"\n          :error=\"categorySurveyValidation.nameCategorySurvey.$error\"\n          v-model=\"nameCategorySurvey\"\n        />\n        <p class=\"text-red-500\" v-if=\"categorySurveyValidation.nameCategorySurvey.$error\">\n          Поле обязательно для заполнения\n        </p>\n        <div class=\"flex space-x-3 mt-3\">\n          <AtButton class=\"w-full p-4\" @click=\"createModalSurveyCategory = false\">Закрыть</AtButton>\n          <AtButton class=\"w-full p-4\" color=\"primary\" :loading=\"btnLoading\" @click=\"addSurveyCategory()\">\n            Добавить\n          </AtButton>\n        </div>\n      </div>\n    </AtModal>\n\n    <AtModal v-model=\"createModalQuestion\" :title=\"'Создать новый опрос'\">\n      <div class=\"mt-2\">\n        <AtInput\n          label=\"Название\"\n          placeholder=\"Введите название опросника\"\n          :error=\"questionValidation.nameQuestion.$error\"\n          v-model=\"nameQuestion\"\n        />\n        <p class=\"text-red-500\" v-if=\"questionValidation.nameQuestion.$error\">Поле обязательно для заполнения</p>\n        <AtSelect\n          label=\"Категория\"\n          :options=\"surveyCategory\"\n          placeholder=\"Выберите категорию опросника\"\n          :error=\"questionValidation.typeQuestion.$error\"\n          v-model=\"typeQuestion\"\n        />\n        <p class=\"text-red-500\" v-if=\"questionValidation.typeQuestion.$error\">Поле обязательно для заполнения</p>\n        <div class=\"flex space-x-3 mt-3\">\n          <AtButton class=\"w-full p-4\" @click=\"createModalQuestion = false\">Закрыть</AtButton>\n          <AtButton class=\"w-full p-4\" color=\"primary\" :loading=\"btnLoading\" @click=\"addQuestion()\">\n            Добавить\n          </AtButton>\n        </div>\n      </div>\n    </AtModal>\n  </div>\n</template>\n\n<style scoped></style>"]}]}