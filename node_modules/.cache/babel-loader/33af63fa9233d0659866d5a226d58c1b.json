 }\n */\nconst addEmployeeVacations = params => apiClient.post(`${END_POINT5}/vacation`, params);\n\nexport {\n  getAllColleagues,\n  allProductivityMetrics,\n  activityColleagues,\n  addEmployeeExtraDayOff,\n  getColleaguesWithoutPagination,\n  addEmployeeVacations,\n};\n"]}]}