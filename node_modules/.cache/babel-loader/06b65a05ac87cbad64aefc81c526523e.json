t storeOfferSalary = (employeeId, params) => apiClient.post(`${END_POINT2}/employee/${employeeId}`, params);\n\nexport {\n  getAllSalariesByParams,\n  showEmployeeIdSalary,\n  storeScheduledSalary,\n  updateScheduledSalaryToActual,\n  editScheduledSalary,\n  storeOfferSalary,\n};\n"]}]}