@param {Object} params - Request params\r\n * @param {boolean} rememberMe\r\n * @returns {Promise<*>}\r\n */\r\n\r\nconst login = params =>\r\n  apiClient.post(`${END_POINT}/login-password`, { ...params }).then(response => {\r\n    setAuth(response);\r\n    store.dispatch('auth/getUser');\r\n  });\r\n\r\nexport { login, getUser };\r\n"]}]}